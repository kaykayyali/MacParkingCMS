<div class="page-header"></div>

	<!-- CONTACT US FORM -->
	<section id="home_contact_us" class="">
		<div class="container ">
			<div class="row">
				<div class="col-xs-12 center-block text-center">
					<h1>Contact Us</h1>
					<br>
					<ul class="list-inline">
						<li>Email:<a href='mailto:macparkingmiami@gmail.com?subject=Interested in your services'> macparkingmiami@gmail.com</a></li>
   						<li>Phone:<a href="tel:786-587-7176"> 786-587-7176</a></li>
   						<li><button class="btn btn-primary btn-xs" id="quick-form">Quick Form</button></li>
   						<script>
   							$('#quick-form').on('click', function() {
   								$('#form_hider').fadeIn();
   							});
   						</script>
					</ul>
					<br>
						<div id="form_hider" style="display: none;">
							<div class="col-xs-12 col-sm-4">
								<div class="form-group">
									<input class="form-control" type="name" placeholder="Name" id="contactName">
								</div>
							</div>
							<div class="col-xs-12 col-sm-4">
								<div class="form-group">
									<input class="form-control" type="email" placeholder="Email" id="contactEmail">
								</div>
							</div>
							<div class="col-xs-12 col-sm-4">
								<div class="form-group">
									<input class="form-control" type="phone" placeholder="Phone" id="contactPhone">
								</div>
							</div>
							<div class="col-xs-12">
								<div class="form-group">
										<textarea class="form-control" rows="3" placeholder="Message" id="contactContent"></textarea>
								</div>
							</div>
							<div class="col-xs-12">
								<button type="submit" class="btn btn-default btn-block" id="contactButton" style="font-size:30px;">Send Message</button>
							</div>
						</div>
				</div>
			</div>
		</div>
	</section>
	<!-- END CONTACT US FORM -->
